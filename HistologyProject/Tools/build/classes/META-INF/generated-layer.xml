<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN"
                            "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Windows2">
        <folder name="Components">
            <file name="ToolsTopComponent.settings">
                <!--com.tools.ToolsTopComponent--><![CDATA[<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE settings PUBLIC "-//NetBeans//DTD Session settings 1.0//EN" "http://www.netbeans.org/dtds/sessionsettings-1_0.dtd">
<settings version="1.0">
  <instance class="com.tools.ToolsTopComponent"/>
</settings>
]]></file>
        </folder>
        <folder name="Modes">
            <folder name="explorer">
                <file name="ToolsTopComponent.wstcref">
                    <!--com.tools.ToolsTopComponent--><![CDATA[<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tc-ref PUBLIC "-//NetBeans//DTD Top Component in Mode Properties 2.0//EN" "http://www.netbeans.org/dtds/tc-ref2_0.dtd">
<tc-ref version="2.0">
  <tc-id id="ToolsTopComponent"/>
  <state opened="true"/>
</tc-ref>
]]></file>
            </folder>
        </folder>
    </folder>
    <folder name="Actions">
        <folder name="Window">
            <file name="com-tools-ToolsTopComponent.instance">
                <!--com.tools.ToolsTopComponent-->
                <attr
                    methodvalue="org.openide.windows.TopComponent.openAction" name="instanceCreate"/>
                <attr name="component" newvalue="com.tools.ToolsTopComponent"/>
                <attr name="preferredID" stringvalue="ToolsTopComponent"/>
                <attr bundlevalue="com.tools.Bundle#CTL_ToolsAction" name="displayName"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="com-tools-ToolsTopComponent.shadow">
                <!--com.tools.ToolsTopComponent-->
                <attr name="originalFile" stringvalue="Actions/Window/com-tools-ToolsTopComponent.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="xml">
        <folder name="entities">
            <folder name="com_tools">
                <file name="Tools" url="nbres:/org/netbeans/modules/settings/resources/properties-1_0.dtd">
                    <!--com.tools.ToolsTopComponent-->
                    <attr name="hint.originalPublicID" stringvalue="-//com.tools//Tools//EN"/>
                </file>
            </folder>
        </folder>
        <folder name="memory">
            <folder name="com">
                <folder name="tools">
                    <file name="ToolsTopComponent">
                        <!--com.tools.ToolsTopComponent-->
                        <attr name="settings.providerPath" stringvalue="xml/lookups//com_tools/Tools.instance"/>
                    </file>
                </folder>
            </folder>
        </folder>
        <folder name="lookups">
            <folder name="com_tools">
                <file name="Tools.instance">
                    <!--com.tools.ToolsTopComponent-->
                    <attr
                        methodvalue="org.netbeans.api.settings.Factory.create" name="instanceCreate"/>
                    <attr
                        methodvalue="org.netbeans.api.settings.Factory.properties" name="settings.convertor"/>
                    <attr name="settings.instanceClass" stringvalue="com.tools.ToolsTopComponent"/>
                    <attr name="settings.instanceOf" stringvalue="com.tools.ToolsTopComponent"/>
                    <attr boolvalue="true" name="xmlproperties.preventStoring"/>
                </file>
            </folder>
        </folder>
    </folder>
</filesystem>
